{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/layout-winter-frost.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
